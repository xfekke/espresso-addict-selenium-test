{
  "name": "espresso-addict",
  "type": "module",
  "author": "Ironboy",
  "version": "1.2.0",
  "description": "A small text adventure - written in vanilla JS",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "test": "cucumber-js tests/features --import tests/features/support --import tests/features/step-definitions --world-parameters {\\\"browserName\\\":\\\"chrome\\\"} --format json:tests/results/chrome.json && node ./generateReport",
    "testOnly": "cucumber-js tests/features --import tests/features/support --import tests/features/step-definitions --world-parameters {\\\"browserName\\\":\\\"chrome\\\"} --format json:tests/results/chrome.json --format @qavajs/console-formatter",
    "testReport": "node ./generateReport",
    "openTestReport": "start tests/results/chrome.html"
  },
  "keywords": [],
  "license": "ISC",
  "dependencies": {
    "@cucumber/cucumber": "^11.0.1",
    "@qavajs/console-formatter": "^0.8.0",
    "express": "^4.21.1",
    "selenium-webdriver": "^4.25.0"
  },
  "devDependencies": {
    "chai": "^5.1.2",
    "cucumber-html-reporter": "^7.2.0",
    "wait-on": "^8.0.1"
  },
  "directories": {
    "test": "tests"
  }
}